{
  "name": "bearodriguez.com",
  "version": "1.0.0",
  "description": "Bea Rodriguez therapy",
  "scripts": {
    "clear": "rm -rf dist",
    "build:eleventy": "npx @11ty/eleventy",
    "build": "npm run clear && npm run build:eleventy",
    "serve": "npx @11ty/eleventy --serve --quiet",
    "start": "npm run clear && npm run serve",
    "staging": "npm run build && echo 'User-agent: *\nDisallow: /' > dist/robots.txt",
    "version": "node scripts/sync-site-version.js",
    "_bump:commit": "git add package*.json src/site/_data/site.json && git commit -m \"Bumps version number to $npm_package_version\"",
    "bump:patch": "npm version patch --no-git-tag-version && npm run _bump:commit",
    "bump:minor": "npm version minor --no-git-tag-version && npm run _bump:commit",
    "bump:major": "npm version major --no-git-tag-version && npm run _bump:commit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tempertemper/eleventy-base.git"
  },
  "author": "Martin Underhill",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tempertemper/eleventy-base/issues"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "esbuild": "^0.24.0",
    "sass": "^1.93.2"
  }
}
